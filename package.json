    "connect-pg-simple": "^10.0.0",
    "memorystore": "^1.6.7",
    "ws": "^8.18.0",
    "openai": "^5.23.1",
    "drizzle-orm": "^0.39.1",
    "drizzle-zod": "^0.7.0",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0",
    "@tanstack/react-query": "^5.60.5",
    "react-hook-form": "^7.55.0",
    "react-icons": "^5.4.0",
    "lucide-react": "^0.453.0",
    "wouter": "^3.3.5",
    "vaul": "^1.1.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.2.5",
    "clsx": "^2.1.1",
